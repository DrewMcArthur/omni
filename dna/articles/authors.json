{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "authorIDs",
    "authorNames"
  ],
  "properties": {
    "authorIDs": {
      "$id": "#/properties/authorIDs",
      "type": "array",
      "title": "The Authorids Schema",
      "items": {
        "$id": "#/properties/authorIDs/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Author1's hash",
          "Author2's hash"
        ],
        "pattern": "^(.*)$"
      }
    },
    "authorNames": {
      "$id": "#/properties/authorNames",
      "type": "array",
      "title": "The Authornames Schema",
      "items": {
        "$id": "#/properties/authorNames/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Author1's title and name ",
          "Author2's title and name"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}